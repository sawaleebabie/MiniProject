e) {\n      console.log(e);\n    }\n  };\n\n  const getCustomer = async (id) => {\n    const result = await axios.get(`${URL}/${id}`)\n    console.log('customers ID: ', result.data)\n    setCustomer(result.data)\n    setCustomerID(result.data.CustomerID)\n    setName(result.data.name)\n    setSurname(result.data.surname)\n    setStatus(result.data.status)\n    setPrice(result.data.price)\n}\n \n  const getCustomers = async () => {\n    let result = await axios.get(URL);\n    setCustomers(result.data.list);\n  };\n\n  const addCustomer = async () => {\n    let result = await axios.post(URL, {\n      CustomerID,\n      name,\n      surname,\n      status,\n      price,\n    });\n    console.log(result);\n    getCustomers();\n  };\n\n  const deleteCustomer = async (id) => {\n    let result = await axios.delete(`${URL}/${id}`);\n    getCustomers();\n  };\n\n  const updateCustomer = async (id) => {\n    let result = await axios.put(`${URL}/${id}`, {\n      CustomerID,\n      name,\n      surname,\n      status,\n      price,\n    });\n    console.log(result);\n    getCustomers();\n  };\n\n  const showCustomers = () => {\n    if (customers && customers.length) {\n      return customers.map((item, index) => {\n        return (\n          <div className={styles.listItem} key={index}>\n            <div><b>CustomerID:</b> {item.CustomerID}</div>\n            <div><b>Name:</b> {item.name}</div>\n            <div><b>Surname:</b> {item.surname}</div>\n            <div><b>Status:</b> {item.status}</div>\n            <div><b>Price:</b> {item.price}</div>\n            <div className={styles.edit_button}>\n              <button className={styles.button_get}\n                onClick={() => getCustomer(item.id)}\n              >\n                Get\n              </button>\n              <button className={styles.button_update}\n                onClick={() => updateCustomer(item.id)}\n              >\n                Update\n              </button>\n              <button className={styles.button_delete}\n                onClick={() => deleteCustomer(item.id)}\n              >\n                Delete\n              </button>\n            </div>\n          </div>\n        );\n      });\n    } else {\n      return <p>Loading...</p>;\n    }\n  };\n  return ( \n    <div>\n      <div>\n        <Image\n          src=\"/laundry.png\"\n          alt=\"Picture\"\n          width={900}\n          height={200}\n          sizes=\"(max-width: 500px) 100px\"\n        />\n      </div>\n      <div className={styles.container}>\n        <Navbar />\n        <h1>For Admin Edit Data</h1>\n        <div className={styles.form}>\n          <tr>\n            <th>\n              <div className={styles.form_add}>\n                <h2>Add customers</h2>\n                CustomerID:\n                <input\n                  type=\"text\"\n                  name=\"CustomerID\"\n                  value = {CustomerID}\n                  onChange={(e) => setCustomerID(e.target.value)}\n                ></input>\n                Name:\n                <input\n                  type=\"text\"\n                  name=\"name\"\n                  value = {name}\n                  onChange={(e) => setName(e.target.value)}\n                ></input>\n                Surname:\n                <input\n                  type=\"text\"\n                  name=\"surname\"\n                  value = {surname}\n                  onChange={(e) => setSurname(e.target.value)}\n                ></input>\n                Status:\n                <input\n                  type=\"text\"\n                  name=\"status\"\n                  value = {status}\n                  onChange={(e) => setStatus(e.target.value)}\n                ></input>\n                Price:\n                <input\n                  type=\"number\"\n                  name=\"price\"\n                  value = {price}\n                  onChange={(e) => setPrice(e.target.value)}\n                ></input>\n                <button className={styles.button_add}onClick={() => addCustomer(CustomerID, name, surname, status, price)}>\n                  Add\n                </button>\n              </div>\n            </th>\n            <div className={styles.textstatusID}><h2>STATUS</h2></div>\n            <div>\n              <Image\n                src=\"/Washing.png\"\n                alt=\"Picture\"\n                width={50}\n                height={50}\n              />\n              <button   className={styles.statusID} onClick= {() => setStatus(\"Washing\") }>\n                Washing\n              </button> <br />\n              <Image\n                src=\"/Rinse.png\"\n                alt=\"Picture\"\n                width={50}\n                height={50}\n              />\n              <button   className={styles.statusID} onClick= {() => setStatus(\"Rinse\") }>\n                Rinse\n              </button> <br />\n              <Image\n                src=\"/AirDry.png\"\n                alt=\"Picture\"\n                width={50}\n                height={50}\n              />\n              <button   className={styles.statusID} onClick= {() => setStatus(\"Air Dry\") }>\n                Air Dry\n              </button> <br />\n              <Image\n                src=\"/Spindryer.png\"\n                alt=\"Picture\"\n                width={50}\n                height={50}\n              />\n              <button   className={styles.statusID} onClick= {() => setStatus(\"Spin dryer\") }>\n                Spin dryer\n              </button> <br />\n              <Image\n                src=\"/Finished.png\"\n                alt=\"Picture\"\n                width={50}\n                height={50}\n              />\n              <button   className={styles.statusFinished} onClick= {() => setStatus(\"Finished\") }>\n                Finished\n              </button>\n            </div>\n          </tr>\n        </div>\n        <div className={styles.list}>{showCustomers()}</div>\n      </div>\n    </div>   \n  );\n};\nexport default withAuth(admin);\n\nexport function getServerSideProps({ req, res }) {\n  return { props: { token: req.cookies.token || \"\" } };\n}\n"]},"metadata":{},"sourceType":"module"}